<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:id="@+id/statusbar"
        android:layout_width="407dp"
        android:layout_height="55dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/iphone_x__or_newer_"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="62dp"
        android:layout_marginTop="124dp"
        android:background="@drawable/bro1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <ImageView
        android:id="@+id/feedbtn"
        android:layout_width="342dp"
        android:layout_height="74dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="557dp"
        android:layout_marginStart="30dp"
        android:background="@drawable/btnfeed"/>
    <ImageView
        android:id="@+id/uploadbtn"
        android:layout_width="342dp"
        android:layout_height="74dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="666dp"
        android:layout_marginStart="30dp"
        android:background="@drawable/primary1"/>

</androidx.constraintlayout.widget.ConstraintLayout>